// Debug decorator test
function logged(target) {
    console.log("1. Decorator called with target type:", typeof target);
    console.log("1. Target keys:", Object.keys(target));
    return target;
}

@logged
class MyClass {
    name: string = "Test";
}

console.log("2. After class definition");

const instance = new MyClass();
console.log("3. After new MyClass()");

// Try to access the constructor
console.log("4. MyClass.prototype:", MyClass.prototype);
console.log("5. instance.__proto__:", instance.__proto__);

// Check if instance was decorated somehow
console.log("6. instance === MyClass.prototype.constructor:", instance === MyClass.prototype.constructor);
