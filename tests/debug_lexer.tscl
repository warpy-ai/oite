console.log("=== Lexer Debug ===");

let tokens = tokenize("1 + 2");
console.log("Token count: " + tokens.length);

let i = 0;
while (i < tokens.length) {
    let tok = tokens[i];
    console.log("  [" + i + "] type=" + tok.type + " value=" + tok.value);
    i = i + 1;
}

console.log("=== Done ===");
