// Detailed lexer debug - manual stepping
console.log("=== Manual Lexer Debug ===");

let src = "42";
let lexer = createLexer(src);

console.log("Source: " + src);
console.log("Source length: " + lexer.source.length);

// Check char code at position 0
let ch0 = lexer.source.charCodeAt(0);
console.log("Char 0 code: " + ch0);
console.log("isDigit(49): " + isDigit(49));
console.log("isDigit(ch0): " + isDigit(ch0));

// Try lexerPeek
let p = lexerPeek(lexer);
console.log("lexerPeek: " + p);
console.log("isDigit(p): " + isDigit(p));

console.log("=== Done ===");
