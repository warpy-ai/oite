// Test new operator support
console.log("Testing new operators...");

// Test typeof
let n = 42;
let s = "hello";
let b = true;
let o = { x: 1 };
let f = function() { return 1; };

console.log("typeof 42: " + typeof n);
console.log("typeof 'hello': " + typeof s);
console.log("typeof true: " + typeof b);
console.log("typeof {}: " + typeof o);
console.log("typeof function: " + typeof f);
console.log("typeof null: " + typeof null);
console.log("typeof undefined: " + typeof undefined);

// Test bitwise operators
let a = 5;  // 0101 in binary
let c = 3;  // 0011 in binary

console.log("");
console.log("Bitwise operators:");
console.log("5 & 3 = " + (a & c));   // Should be 1 (0001)
console.log("5 | 3 = " + (a | c));   // Should be 7 (0111)
console.log("5 ^ 3 = " + (a ^ c));   // Should be 6 (0110)
console.log("5 << 1 = " + (a << 1)); // Should be 10 (1010)
console.log("5 >> 1 = " + (a >> 1)); // Should be 2 (0010)

// Test delete
let obj = { x: 1, y: 2, z: 3 };
console.log("");
console.log("Delete operator:");
console.log("Before delete: x=" + obj.x + ", y=" + obj.y);
delete obj.y;
console.log("After delete obj.y: x=" + obj.x + ", y=" + obj.y);

console.log("");
console.log("Operator tests complete!");
